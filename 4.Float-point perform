#include<iostream>
using namespace std;
int main(){
	float num1,num2;
	cout<<"enter a number1: ";
	cin>>num1;
	cout<<"enter a number2:";
	cin>>num2;
	cout<<"sum:"<<num1+num2<<endl;
	cout<<"subtract:"<<num1-num2<<endl;
	cout<<"multiplication:"<<num1*num2<<endl;
	cout<<"divison:"<<(float)num1/num2<<endl;

}
