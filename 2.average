#include <iostream>

int main() {
  int num1, num2, num3;
  double average;

  cout << "Enter first number: ";
  cin >> num1;
  cout << "Enter second number: ";
  cin >> num2;
  cout << "Enter third number: ";
  cin >> num3;

  average = (double)(num1 + num2 + num3) / 3;
  cout << "Average: " << average <<endl;

  return 0;
}
